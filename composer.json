{
    "name": "seven-ario/laraish_framework",
    "description": "The Laraish Framework for WordPress.",
    "keywords": [
        "framework",
        "laravel",
        "wordpress"
    ],
    "license": "MIT",
    "homepage": "http://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "yaquawa",
            "email": "yaquawa@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "Foundation/helpers.php"
        ],
        "psr-4": {
            "Laraish\\": "./"
        }
    },
    "scripts": {
        "fix-cs": ["prettier **/*.php --write"]
    },
    "repositories": {
		"seven-ario/laraish_contracts": {
		    "type": "vcs",
		    "url": "https://github.com/seven-ario/laraish_contracts.git"
		},
        "seven-ario/laraish_support": {
		    "type": "vcs",
		    "url": "https://github.com/seven-ario/laraish_support.git"
        },
        "seven-ario/laraish_pagination": {
		    "type": "vcs",
		    "url": "https://github.com/seven-ario/laraish_pagination.git"
	    },
        "seven-ario/laraish_options": {
		    "type": "vcs",
	        "url": "https://github.com/seven-ario/laraish_options.git"
	    }
	},
    "require": {
        "php": "^8.1.9",
        "laravel/framework": "^9.0",
        "seven-ario/laraish_contracts": "dev-master",
        "seven-ario/laraish_support": "dev-master",
        "seven-ario/laraish_pagination": "dev-master",
        "seven-ario/laraish_options": "dev-master"
    }
}
